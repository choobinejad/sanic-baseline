[ ca ]
default_ca = my_ca

[ my_ca ]
serial = certs/serial
database = certs/index.txt
new_certs_dir = ./certs
certificate = certs/ca.crt
private_key = certs/ca.key
default_md = sha256
default_days = 3650
policy = my_policy

[ my_policy ]
countryName = match
stateOrProvinceName = supplied
organizationName = supplied
commonName = supplied
organizationalUnitName = optional
commonName = supplied

