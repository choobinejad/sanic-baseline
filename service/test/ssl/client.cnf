[ req ]
default_bits = 4096
default_keyfile = certs/client.key
encrypt_key = no
default_md = sha256
prompt = no
utf8 = yes
distinguished_name = my_req_distinguished_name
req_extensions = my_extensions

[ my_req_distinguished_name ]
C = US
ST = MD
L = MoCo
O  = sanic-baseline
CN = sanic-baseline-client

[ my_extensions ]
basicConstraints=CA:FALSE
subjectKeyIdentifier = hash
